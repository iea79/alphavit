block variable
    -var frontVersion = '1.2';
    -var email = 'info@sitename.ru';
    -var phone = '+7 (495) 569 63 63';

<!DOCTYPE html>
html(lang="en")
head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title Document

    link(rel="stylesheet", type="text/css", href="css/style.css?"+frontVersion+"")
body
    header.header
        .container_center
            .header__top
                .header__left
                    .navbar__toggle
                    a.logo(href="/")
                        img(src="img/logo.svg", alt="АЛФАВИТ - Лизинговое агенство")
                        span.logo__text АЛФАВИТ
                            small Лизинговое агенство
                    .logo__subtitle Экономим ваши деньги и время!

                .header__contact
                    .header__contactItem
                        a.email(href="mailto: "+email+"")= email
                    .header__contactItem
                        a.phone(href="tel: "+phone+"")
                            b= phone
                    .header__contactItem.desktop
                        button.btn.btn_outline.btn_contrast(type="button" data-toggle="modal" data-target="#callback") Заказать звонок

            .header__bottom
                .navbar__toggle
                nav.header__nav
                    ul.navbar
                        -var nav = [{name: 'Почему мы', anchor: 'about'},{name: 'Акции', anchor: 'sell'},{name: 'Преимущества', anchor: 'profit'},{name: 'Схема работы ', anchor: 'works'},{name: 'Партнёры', anchor: 'partners'},{name: 'Контакты', anchor: 'contact'}];
                        each item in nav
                            li.navbar__item
                                a.navbar__link(href="#"+item.anchor+"" data-scroll-to)= item.name
                .header__action
                    button.btn.btn_plus(type="button" data-toggle="modal" data-target="#callback") Подать заявку

    main.main_content
        // begin home
        section#home.home
            .homePanes
                -var homePane = [{color: '#0159dd', title: 'Лизинг', sub: 'легковых автомобилей', list: ['от 500 000 руб.','до 5 лет','от 5%','от 5% годовых','3 рабочих дня'], img: 'home-auto.png'},{color: '#323337', title: 'Лизинг', sub: 'Грузового транспорта', list: ['от 500 000 руб.','до 5 лет','от 5%','от 5% годовых','3 рабочих дня'], img: 'home-cargo.png'},{color: '#f8b200', title: 'Лизинг', sub: 'спецтехники', list: ['от 500 000 руб.','до 5 лет','от 5%','от 5% годовых','3 рабочих дня'], img: 'home-spectech.png'},{color: '#b5536a', title: 'Лизинг', sub: 'оборудования', list: ['от 500 000 руб.','до 5 лет','от 20%','от 7% годовых','3-5 рабочих дней'], img: 'home-factory.png'},{color: '#d01103', title: 'Лизинг', sub: 'Воздушного транспорта', list: ['от 5 000 000 руб.','до 5 лет','от 20%','от 5% годовых','5-7 рабочих дней'], img: 'home-helicopter.png'},{color: '#063675', title: 'Лизинг', sub: 'водного транспорта', list: ['от 5 000 000 руб.','до 5 лет','от 20%','от 5% годовых','1-2 недели'], img: 'home-yacht.png'},{color: '#0d1730', title: 'Лизинг', sub: 'Коммерческой недвижимости', list: ['от 5 000 000 руб.','до 5 лет','от 20%','от 5% годовых','5-7 рабочих дней'], img: 'home-building.png'},{color: '#6b5f88', title: 'Возвратный лизинг', sub: '', list: ['от 1 000 000 руб.','до 5 лет','от 0%','от 5% годовых','3-5 рабочих дней'], img: 'home-money.png'}];
                each item, i in homePane
                    case i
                        when 0
                            -var actClass = ' active'
                        default
                            -var actClass = ''
                    .homePanes__item(id="home-"+i+"" class= actClass)
                        .homePanes__bg(style="background-color: "+item.color+"")
                        .homePanes__bgText Лизинг
                        .homePanes__content.container_center
                            .homePanes__descr
                                .homePanes__title= item.title
                                .homePanes__sub= item.sub
                                .homePanes__list
                                    each value, count in item.list
                                        .homePanes__listRow
                                            case count
                                                when 0: .homePanes__listLabel Стоимость
                                                when 1: .homePanes__listLabel Срок
                                                when 2: .homePanes__listLabel Авансовый платёж
                                                when 3: .homePanes__listLabel Удорожание в год
                                                when 4: .homePanes__listLabel Срок рассмотрения
                                            .homePanes__listValue= value
                            .homePanes__img
                                img(src="img/"+item.img+"", alt="")

            .container_center
                .homeTabs
                    -var homeTabs = [{img: 'tab-car', name: 'Легковые автомобили'},{img: 'tab-cargo', name: 'Грузовые автомобили'},{img: 'tab-spectech', name: 'Спецтехника'},{img: 'tab-manufacture', name: 'Оборудование'},{img: 'tab-helicopter', name: 'Воздушный транспорт'},{img: 'tab-water', name: 'Водный транспорт'},{img: 'tab-build', name: 'Недвижимость'},{img: 'tab-money', name: 'Возвратный лизинг'}];
